    <thead class="thead-dark">
      <tr>
        <th scope="col">#</th>
        <th scope="col">Име</th>
        <th scope="col">Pari</th>
      </tr>
    </thead>
    <tbody>
      {% for key,item in auth.user.topPlyers('pari',5) %}
      <tr>
        <td scope="row">{{key + 1}}</td>
        <td>
        <img src="{{auth.user.get_gravatar(auth.user.getEmail(item.user_id),40)}}" width="40" height="40" style="border-radius:50%;"> {{auth.user.getName(item.user_id)}}
        </td>
        <td>
          {{item.pari}}$
        </td>
      </tr>
      {% endfor %}
    </tbody>
  </table>
</div>
